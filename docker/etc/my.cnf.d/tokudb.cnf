[mariadb]
# See https://mariadb.com/kb/en/how-to-enable-tokudb-in-mariadb/
# for instructions how to enable TokuDB## See https://mariadb.com/kb/en/tokudb-differences/ for differences
#  between TokuDB in MariaDB and TokuDB from http://www.tokutek.com/
plugin-load-add=ha_tokudb.so
tokudb_data_dir=/var/lib/mysql/tokudb
tokudb_tmp_dir=/opt/temp/tokudb
